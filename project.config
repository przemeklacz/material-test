[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/przemeklacz
[access "refs/for/refs/*"]
	read = group user/przemeklacz
	push = group user/przemeklacz
	label-Code-Review = -2..+2 group user/przemeklacz
	label-Verified = -1..+1 group user/przemeklacz
	rebase = group user/przemeklacz
	removeReviewer = group user/przemeklacz
	submit = group user/przemeklacz
[access "refs/heads/*"]
	read = group user/przemeklacz
	create = group user/przemeklacz
	push = +force group user/przemeklacz
	pushMerge = group user/przemeklacz
[access "refs/tags/*"]
	push = group user/przemeklacz
	push = +force group user/przemeklacz
